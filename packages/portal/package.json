{
  "name": "component-store-portal",
  "version": "0.1.0",
  "private": true,
  "description": "Portal for component-store.",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 UMI_ENV=prod max build",
    "build": "cross-env UMI_ENV=prod max build --max_old_space_size=4096 && npm run build:menu",
    "build:all": "pnpm build && ./build.sh",
    "build:menu": "node ./config/menu/index.mjs",
    "build:menu-watch": "cross-env WATCH=1 MENU_JSON_PATH=../../public/menu.json node ./config/menu/index.mjs",
    "dev": "concurrently \"max dev\" \"npm run build:menu-watch\"",
    "postinstall": "max setup",
    "lint": "npm run lint:es && npm run lint:style && npm run lint:prettier",
    "lint-fix": "npm run lint-fix:es && npm run lint-fix:style",
    "lint-fix:es": "eslint --ext .jsx,.js,.tsx,.ts src --fix",
    "lint-fix:style": "stylelint \"src/{pages,components,assets}/**/*.less\" --syntax less --fix",
    "lint-staged": "lint-staged",
    "lint-staged:es": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:es": "eslint --ext .jsx,.js,.tsx,.ts src",
    "lint:prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "lint:style": "stylelint \"src/{pages,components,assets}/**/*.less\" --syntax less",
    "start": "max dev",
    "test": "jest",
    "test:coverage": "max g jest --coverage"
  },
  "dependencies": {
    "@alilc/lowcode-datasource-engine": "^1.0.0",
    "@alilc/lowcode-datasource-fetch-handler": "latest",
    "@ant-design/icons": "^5.0.0",
    "@ant-design/pro-components": "^2.3.52",
    "@formily/antd": "2.2.4",
    "@formily/core": "2.2.4",
    "@formily/react": "2.2.4",
    "@tenx-ui/auth-utils": "0.0.1",
    "@tenx-ui/bff-client": "0.1.2",
    "@tenx-ui/charts": "1.1.2",
    "@tenx-ui/component-store-bff-client": "0.0.33",
    "@tenx-ui/editor": "3.0.2-beta.1",
    "@tenx-ui/form-helper": "0.0.2",
    "@tenx-ui/icon": "^2.4.28",
    "@tenx-ui/icon-materials": "0.1.4",
    "@tenx-ui/materials": "1.5.12-beta.0",
    "@tenx-ui/react-markdown-lowcode-materials": "0.1.4",
    "@tenx-ui/request": "^1.0.0-beta.6",
    "@tenx-ui/utils": "^2.3.1",
    "@umijs/max": "^4.0.42",
    "antd": "5.6.4",
    "babel-runtime": "^6.26.0",
    "classnames": "^2.2.5",
    "dayjs": "^1.11.7",
    "graphql": "^16.6.0",
    "graphql-request": "^5.1.0",
    "graphql-tag": "^2.12.6",
    "query-string": "^8.1.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "remark-gfm": "^4.0.0",
    "shared-components": "workspace:*",
    "swr": "^2.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13",
    "@types/classnames": "^2.2.7",
    "@types/express": "^4.17.0",
    "@types/history": "^4.7.2",
    "@types/jest": "^27",
    "@types/lodash": "^4.14.144",
    "@types/qs": "^6.5.3",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "concurrently": "^7.6.0",
    "cross-env": "^5.1.4",
    "jest": "^27",
    "typescript": "^4.1.2"
  },
  "packageManager": "pnpm",
  "engines": {
    "node": ">=18.12.0",
    "pnpm": ">=8.1.0"
  }
}
